{
  "name": "tulp",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "scramantha",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/main.scss static/main.css",
    "watch-css-main": "nodemon --watch scss/ --ext scss --exec \"npm run css-compile\""
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "node": "^9.11.2",
    "nuxt": "^1.4.2",
    "nuxt-i18n": "^4.1.0",
    "vue-i18n": "^7.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.7.1",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "sass-loader": "^7.1.0"
  }
}
